{
    "addFromDashboard": {
        "addBoard": "div[class='vs-c-my-organizations-item-wrapper']>div:nth-last-of-type(3) li[title='Add new Board']"
    },
    "addFromSidebarUp": {
        "addNewSomething": ".vb-content > div.vs-c-list > .vs-c-list__item > .vs-c-list__btn",
        "addOrganization": ".vs-c-list > :nth-child(1) > a"
    },
    "addFromSidebarDown": {
        "addToNewOrg": "li[class='vs-c-list__item has-caret']:last-of-type li",
        "addBoardToNewOrg": "li > a"
    },
    "newBoard": {
        "organization": "ul[class='el-scrollbar__view el-select-dropdown__list']>li:last-of-type",
        "title": "input[name='name']"
    },
    "boardType": {
        "scrum": "span[name='type_scrum']",
        "kanban": "span[name='type_kanban']"
    },
    "import": {
        "configuration": ":nth-child(1) > .vs-input-border > .el-select > .el-input > .el-input__inner",
        "members": ":nth-child(2) > .vs-input-border > .el-select > .el-input > .el-input__inner"
    },
    "logo": {
        "uploadlogo": "input[name='file']",
        "closeLogoModal": "button[name='close-confirmation-modal-btn']",
        "deleteLogo": ".vs-u-pull--left",
        "cancelLogo": "[name='cancel-btn']",
        "saveLogo": ".vs-u-text--right > .el-button--success"
    },
    "navigation": {
        "previousButton": "button[name='prev_btn']",
        "nextButton": "button[name='next_btn']",
        "closeModal": "button[name='close-new-board-modal-btn']"
    },
    "visitNewBoardFromSideBar": "li[class='vs-c-list__item has-caret']:last-of-type li:nth-last-child(2)"
}
